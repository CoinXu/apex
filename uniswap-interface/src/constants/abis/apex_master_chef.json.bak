;;;;;;;;;;14081;14086:1;14081:7;;;;;;;;;;;;;:23;;;;;;;;;;;14122:4;14115:11;;;13908:225;:::o;877:203:34:-;977:96;997:5;1027:27;;;1056:4;1062:2;1066:5;1004:68;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;977:19;:96::i;:::-;877:203;;;;:::o;3713:272:31:-;3799:7;3830:1;3826;:5;3833:12;3818:28;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;3856:9;3872:1;3868;:5;;;;;;3856:17;;3977:1;3970:8;;;3713:272;;;;;:::o;1746:187::-;1832:7;1864:1;1859;:6;;1867:12;1851:29;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;1890:9;1906:1;1902;:5;1890:17;;1925:1;1918:8;;;1746:187;;;;;:::o;2959:751:34:-;3378:23;3404:69;3432:4;3404:69;;;;;;;;;;;;;;;;;3412:5;3404:27;;;;:69;;;;;:::i;:::-;3378:95;;3507:1;3487:10;:17;:21;3483:221;;;3627:10;3616:30;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;3608:85;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;3483:221;2959:751;;;:::o;3573:194:35:-;3676:12;3707:53;3730:6;3738:4;3744:1;3747:12;3707:22;:53::i;:::-;3700:60;;3573:194;;;;;:::o;4920:958::-;5050:12;5082:18;5093:6;5082:10;:18::i;:::-;5074:60;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;5205:12;5219:23;5246:6;:11;;5266:8;5277:4;5246:36;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;5204:78;;;;5296:7;5292:580;;;5326:10;5319:17;;;;;;5292:580;5457:1;5437:10;:17;:21;5433:429;;;5695:10;5689:17;5755:15;5742:10;5738:2;5734:19;5727:44;5644:145;5834:12;5827:20;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;4920:958;;;;;;;:::o;718:413::-;778:4;981:12;1090:7;1078:20;1070:28;;1123:1;1116:4;:8;1109:15;;;718:413;;;:::o",
  "source": "pragma solidity 0.6.12;\n\n\nimport \"@openzeppelin/contracts/token/ERC20/IERC20.sol\";\nimport \"@openzeppelin/contracts/token/ERC20/SafeERC20.sol\";\nimport \"@openzeppelin/contracts/utils/EnumerableSet.sol\";\nimport \"@openzeppelin/contracts/math/SafeMath.sol\";\nimport \"@openzeppelin/contracts/access/Ownable.sol\";\nimport \"./uniswapv2/interfaces/IWETH.sol\";\nimport \"./uniswapv2/interfaces/IUniswapV2Factory.sol\"; // interface factorys\nimport \"./uniswapv2/interfaces/IUniswapV2Router02.sol\"; // interface factorys\n\nimport \"./ApexToken.sol\";\nimport \"./uniswapv2/interfaces/IUniswapV2Pair.sol\";\n\n\ncontract MasterChef is Ownable {\n    using SafeMath for uint256;\n    using SafeERC20 for IERC20;\n\n    // Info of each user.\n    struct UserInfo {\n        uint256 amount;     // How many LP tokens the user has provided.\n        uint256 rewardDebt; // Reward debt. See explanation below.\n        //\n        // We do some fancy math here. Basically, any point in time, the amount of SUSHIs\n        // entitled to a user but is pending to be distributed is:\n        //\n        //   pending reward = (user.amount * pool.accSushiPerShare) - user.rewardDebt\n        //\n        // Whenever a user deposits or withdraws LP tokens to a pool. Here's what happens:\n        //   1. The pool's `accSushiPerShare` (and `lastRewardBlock`) gets updated.\n        //   2. User receives the pending reward sent to his/her address.\n        //   3. User's `amount` gets updated.\n        //   4. User's `rewardDebt` gets updated.\n    }\n\n    // Info of each pool.\n    struct PoolInfo {\n        IERC20 poolToken;           // Address of LP token contract.\n        uint256 allocPoint;       // How many allocation points assigned to this pool.\n        uint256 lastRewardBlock;  // Last block number that APEX distribution occurs.\n        uint256 accApexPerShare; // Accumulated APEX per share, times 1e12. See below.\n    }\n\n    ApexToken public apex;\n\n    IUniswapV2Router02 public uniswapRouterV2;\n    IUniswapV2Factory public uniswapFactory;\n\n    address public tokenUniswapPair;\n\n    bool public LPGenerationCompleted;\n\n    // reward decrease period time\n    uint256 public constant DURATION = 1 weeks;\n    uint256 public constant TOTAL_DURATION = 6 weeks;\n    // initial amount of doki\n    uint256 public initReward = 2555 * 1e18;\n    bool public haveStarted = false;\n    // next time of reward decrease\n    uint256 public decreaseRewardTime = 0;\n    uint256 public stableRewardStartTime = 0;\n    uint256 public decreaseRewardRate = 1;\n    uint256 public lastUpdateTime = 0;\n    uint256 public lastDistributeTime = 0;\n    // distribution of per second\n    uint256 public rewardRate = 0;\n    uint256 public rewardPerLPToken = 0;\n\n    // avg ethereum block per second\n    uint256 public constant BLOCK_PER_DAY = 6450;\n    uint256 public constant SECONDS_PER_DAY = 86400;\n\n    uint256 public totalLPTokensMinted;\n    uint256 public totalWhitelistContributed;\n    uint256 public LPperETHUnit;\n\n    // Info of each pool.\n    PoolInfo[] public poolInfo;\n    // Info of each user that stakes LP tokens.\n    mapping (uint256 => mapping (address => UserInfo)) public userInfo;\n    mapping (address => uint)  public whitelistContributed;\n    mapping (address => bool) public LPGClaimed;\n    // Total allocation poitns. Must be the sum of all allocation points in all pools.\n    uint256 public totalAllocPoint = 0;\n\n    event Start(uint amount);\n    event RewardDecrease(uint amount);\n    event Deposit(address indexed user, uint256 indexed pid, uint256 amount);\n    event Withdraw(address indexed user, uint256 indexed pid, uint256 amount);\n    event EmergencyWithdraw(address indexed user, uint256 indexed pid, uint256 amount);\n    event Transfer(address indexed from, address indexed to, uint256 value);\n    event LiquidityAddition(address indexed dst, uint value);\n    event LPTokenClaimed(address dst, uint value);\n\n    constructor(\n        ApexToken _apex\n    ) public {\n        apex = _apex;\n        initialSetup();\n    }\n\n    function initialSetup() internal {\n        uniswapRouterV2 = IUniswapV2Router02(0x7a250d5630B4cF539739dF2C5dAcb4c659F2488D); // For testing\n        uniswapFactory = IUniswapV2Factory(0x5C69bEe701ef814a2B6a3EDD4B1652CB9cc5aA6f); // For testing\n        createUniswapPairMainnet();\n    }\n\n    function createUniswapPairMainnet() public returns (address) {\n        require(tokenUniswapPair == address(0), \"Token: pool already created\");\n        tokenUniswapPair = uniswapFactory.createPair(\n            address(uniswapRouterV2.WETH()),\n            address(apex)\n        );\n        return tokenUniswapPair;\n    }\n\n    function poolLength() external view returns (uint256) {\n        return poolInfo.length;\n    }\n\n    function startFarming() external onlyOwner {\n        rewardRate = initReward.div(DURATION);\n        apex.mint(address(this), initReward);\n\n        lastUpdateTime = block.timestamp;\n        lastDistributeTime = block.timestamp;\n        decreaseRewardTime = block.timestamp.add(DURATION);\n        stableRewardStartTime = block.timestamp.add(TOTAL_DURATION);\n\n        haveStarted = true;\n        emit Start(initReward);\n    }\n\n    function checkRewardDecrease() internal {\n        if (stableRewardStartTime > block.timestamp && block.timestamp >= decreaseRewardTime) {\n            initReward = initReward.mul(100 - decreaseRewardRate * 10).div(100);\n            decreaseRewardRate += 1;\n            apex.mint(address(this), initReward);\n\n            rewardRate = initReward.div(DURATION);\n            decreaseRewardTime = decreaseRewardTime.add(DURATION);\n\n            emit RewardDecrease(initReward);\n        }\n    }\n\n    // Add a new token to the pool. Can only be called by the owner.\n    // XXX DO NOT add the same token more than once. Rewards will be messed up if you do.\n    function add(uint256 _allocPoint, IERC20 _poolToken, bool _withUpdate) public onlyOwner {\n        if (_withUpdate) {\n            massUpdatePools();\n        }\n        uint256 lastRewardBlock = block.number;\n        totalAllocPoint = totalAllocPoint.add(_allocPoint);\n        poolInfo.push(PoolInfo({\n        poolToken: _poolToken,\n        allocPoint: _allocPoint,\n        lastRewardBlock: lastRewardBlock,\n        accApexPerShare: 0\n        }));\n    }\n\n\n    function pendingApex(uint256 _pid, address _user) external view returns (uint256) {\n        PoolInfo storage pool = poolInfo[_pid];\n        UserInfo storage user = userInfo[_pid][_user];\n        uint256 accApexPerShare = pool.accApexPerShare;\n        uint256 lpSupply = pool.poolToken.balanceOf(address(this));\n        if (block.number > pool.lastRewardBlock && lpSupply != 0) {\n            uint256 numBlocks = block.number.sub(pool.lastRewardBlock);\n            uint256 rewardPerBlock = rewardRate.mul(SECONDS_PER_DAY).div(BLOCK_PER_DAY);\n            uint256 apexReward = numBlocks.mul(rewardPerBlock).mul(pool.allocPoint).div(totalAllocPoint);\n            accApexPerShare = accApexPerShare.add(apexReward.mul(1e12).div(lpSupply));\n        }\n        return user.amount.mul(accApexPerShare).div(1e12).sub(user.rewardDebt);\n    }\n\n    // Update reward variables for all pools. Be careful of gas spending!\n    function massUpdatePools() public {\n        uint256 length = poolInfo.length;\n        for (uint256 pid = 0; pid < length; ++pid) {\n            updatePool(pid);\n        }\n    }\n\n    // Update reward variables of the given pool to be up-to-date.\n    function updatePool(uint256 _pid) public {\n        PoolInfo storage pool = poolInfo[_pid];\n        if (block.number <= pool.lastRewardBlock) {\n            return;\n        }\n        uint256 tokenSupply = pool.poolToken.balanceOf(address(this));\n        if (tokenSupply == 0) {\n            pool.lastRewardBlock = block.number;\n            return;\n        }\n        uint256 numBlocks = block.number.sub(pool.lastRewardBlock);\n        uint256 rewardPerBlock = rewardRate.mul(SECONDS_PER_DAY).div(BLOCK_PER_DAY);\n        uint256 apexReward = numBlocks.mul(rewardPerBlock).mul(pool.allocPoint).div(totalAllocPoint);\n        pool.accApexPerShare = pool.accApexPerShare.add(apexReward.mul(1e12).div(tokenSupply));\n        pool.lastRewardBlock = block.number;\n    }\n\n\n    function depositWhitelist(address referrerAddress) public payable {\n        require(referrerAddress != address(0), \"Invalid referrer address\");\n        require(1<= msg.value && msg.value <= 5, \"Contributed value is invalid\");\n        whitelistContributed[msg.sender] += msg.value; // Overflow protection from safemath is not needed here\n        require(whitelistContributed[msg.sender] <= 5, \"Whitelist contribution amount exceeds maximum\");\n\n        if(LPGenerationCompleted == false) {\n            totalWhitelistContributed = totalWhitelistContributed.add(msg.value); // for front end display during LGE\n            emit LiquidityAddition(msg.sender, msg.value);\n            if (totalWhitelistContributed >= 1000) {\n                liquidityGenerationUniswapENCORExWETHPair();\n            }\n        } else {\n            IUniswapV2Pair pair = IUniswapV2Pair(tokenUniswapPair);\n            uint ethReceived = msg.value;\n            uint ethToConvert = ethReceived.div(2);\n            uint ethToTransfer = ethReceived - ethToConvert;\n            uint[] memory amounts = uniswapRouterV2.swapExactETHForTokens{value: ethToConvert}(0,\n                getPathForETHToApex(), address(this), block.timestamp+15);\n            uint apexAmount = amounts[amounts.length - 1];\n            address WETH = uniswapRouterV2.WETH();\n            IWETH(WETH).deposit{value : ethToTransfer}();\n            safeApexTransfer(address(pair), apexAmount);\n            pair.mint(msg.sender);\n            totalLPTokensMinted = pair.balanceOf(msg.sender);\n            require(totalLPTokensMinted != 0 , \"LP creation failed\");\n        }\n    }\n\n    function liquidityGenerationUniswapENCORExWETHPair() public {\n        require(LPGenerationCompleted == false, \"Liquidity generation already finished\");\n        totalWhitelistContributed = address(this).balance;\n        IUniswapV2Pair pair = IUniswapV2Pair(tokenUniswapPair);\n\n        //Wrap eth\n        address WETH = uniswapRouterV2.WETH();\n        IWETH(WETH).deposit{value : totalWhitelistContributed}();\n        require(address(this).balance == 0 , \"Transfer Failed\");\n        IWETH(WETH).transfer(address(pair),totalWhitelistContributed);\n        emit Transfer(address(this), address(pair), apex.balanceOf(address(this)));\n        safeApexTransfer(address(pair), apex.balanceOf(address(this)));\n        pair.mint(address(this));\n        totalLPTokensMinted = pair.balanceOf(address(this));\n\n        require(totalLPTokensMinted != 0 , \"LP creation failed\");\n        LPperETHUnit = totalLPTokensMinted.mul(1e18).div(totalWhitelistContributed); // 1e18x for  change\n\n        require(LPperETHUnit != 0 , \"LP creation failed\");\n        LPGenerationCompleted = true;\n\n    }\n\n    function claimLPTokens() public {\n        require(LPGenerationCompleted, \"Event not over yet\");\n        require(whitelistContributed[msg.sender] > 0 , \"Nothing to claim, move along\");\n        require(LPGClaimed[msg.sender] == false, \"You have already claimed\");\n        IUniswapV2Pair pair = IUniswapV2Pair(tokenUniswapPair);\n        uint256 amountLPToTransfer = whitelistContributed[msg.sender].mul(LPperETHUnit).div(1e18);\n        pair.transfer(msg.sender, amountLPToTransfer); // stored as 1e18x value for change\n        LPGClaimed[msg.sender] == true;\n        emit LPTokenClaimed(msg.sender, amountLPToTransfer);\n    }\n\n    // Deposit tokens for APEX allocation.\n    function deposit(uint256 _pid, uint256 _amount) public {\n        require(_amount > 0, \"Deposit amount is invalid!\");\n        PoolInfo storage pool = poolInfo[_pid];\n        UserInfo storage user = userInfo[_pid][msg.sender];\n        updatePool(_pid);\n        if (user.amount > 0) {\n            uint256 pending = user.amount.mul(pool.accApexPerShare).div(1e12).sub(user.rewardDebt);\n            if(pending > 0) {\n                safeApexTransfer(msg.sender, pending);\n            }\n        }\n        checkRewardDecrease();\n        pool.poolToken.safeTransferFrom(address(msg.sender), address(this), _amount);\n        user.amount = user.amount.add(_amount);\n\n        user.rewardDebt = user.amount.mul(pool.accApexPerShare).div(1e12);\n        emit Deposit(msg.sender, _pid, _amount);\n    }\n\n    // Withdraw staked tokens.\n    function withdraw(uint256 _pid, uint256 _amount) public {\n        PoolInfo storage pool = poolInfo[_pid];\n        UserInfo storage user = userInfo[_pid][msg.sender];\n        require(user.amount >= _amount, \"withdraw: not good\");\n        updatePool(_pid);\n        uint256 pending = user.amount.mul(pool.accApexPerShare).div(1e12).sub(user.rewardDebt);\n        if(pending > 0) {\n            safeApexTransfer(msg.sender, pending);\n        }\n        checkRewardDecrease();\n        if(_amount > 0) {\n            user.amount = user.amount.sub(_amount);\n            pool.poolToken.safeTransfer(address(msg.sender), _amount);\n        }\n        user.rewardDebt = user.amount.mul(pool.accApexPerShare).div(1e12);\n        emit Withdraw(msg.sender, _pid, _amount);\n    }\n\n    // Withdraw without caring about rewards. EMERGENCY ONLY.\n    function emergencyWithdraw(uint256 _pid) public {\n        PoolInfo storage pool = poolInfo[_pid];\n        UserInfo storage user = userInfo[_pid][msg.sender];\n        pool.poolToken.safeTransfer(address(msg.sender), user.amount);\n        emit EmergencyWithdraw(msg.sender, _pid, user.amount);\n        user.amount = 0;\n        user.rewardDebt = 0;\n    }\n\n    // Safe APEX transfer function, just in case if rounding error causes pool to not have enough APEX.\n    function safeApexTransfer(address _to, uint256 _amount) internal {\n        uint256 apexBal = apex.balanceOf(address(this));\n        if (_amount > apexBal) {\n            apex.transfer(_to, apexBal);\n        } else {\n            apex.transfer(_to, _amount);\n        }\n    }\n\n    function getPathForETHToApex() private view returns (address[] memory) {\n        address[] memory path = new address[](2);\n        path[0] = uniswapRouterV2.WETH();\n        path[1] = address(apex);\n\n        return path;\n    }\n\n}\n",
